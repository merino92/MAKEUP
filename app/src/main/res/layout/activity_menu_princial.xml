<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".menu_princial">

    <include layout="@layout/content_menu_princial" android:id="@+id/include" android:layout_width="match_parent"
             android:layout_height="match_parent"/>
    <android.support.design.widget.BottomNavigationView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:menu="@menu/nav_items"
            android:background="#E6E6FA"
            app:elevation="10dp" android:layout_alignParentBottom="true" android:layout_marginBottom="0dp"
            android:layout_marginTop="21dp" android:id="@+id/navegador" android:layout_marginStart="0dp"
            app:layout_anchorGravity="bottom|center" app:layout_anchor="@+id/include"/>

</android.support.design.widget.CoordinatorLayout>